<html>
	<head>
		<title><%= title %></title>
	</head>
	<body>
		<a href="/new">
			<h3>Submit New Message</h3>
		</a>
		<ul>
			<% messages.forEach((message) => { %>
			<li>User: <%= message.user %></li>
			<li>Message: <%= message.text %></li>
			<li>Added: <%= message.added %></li>
			<% }); %>
		</ul>
	</body>
</html>
